{
  "name": "blog-api",
  "version": "1.0.0",
  "description": "Blog API",
  "type": "module",
  "main": "server.js",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "migration:create": "node umzug/migrate create --name",
    "migrate": "node umzug/migrate up",
    "migrate:status": "node umzug/migrateStatus.js",
    "migrate:rollback": "node umzug/migrate down",
    "migrate:rollback:all": "node  umzug/migrate down --to 0",
    "seed": "node umzug/seed up",
    "seed:status": "node umzug/seedStatus.js",
    "seed:create": "node umzug/seed create --name",
    "seed:rollback": "node umzug/seed down",
    "seed:rollback:all": "node umzug/seed down --to 0",
    "lint": "eslint .",
    "format": "npx prettier --write .",
    "dev": "nodemon server.js",
    "start": "node server.js"
  },
  "author": "Ahmad AbuDawaba",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.721.0",
    "@aws-sdk/client-s3": "^3.722.0",
    "@aws-sdk/cloudfront-signer": "^3.709.0",
    "bcryptjs": "^2.4.3",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "isomorphic-dompurify": "^2.19.0",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "jsdom": "^25.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": ">=2.0.1",
    "mysql2": "^3.12.0",
    "sequelize": "^6.37.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-cloudwatch": "^6.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.9",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.9",
    "umzug": "^3.8.2"
  }
}