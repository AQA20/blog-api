{
  "parser": "@babel/eslint-parser",
  "env": {
    "node": true,
    "es2023": true
  },
  "parserOptions": {
    "ecmaVersion": 2023,
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "quotes": ["warn", "single"],
    "no-unused-vars": "warn"
  },
  "ignorePatterns": ["node_modules/", "test.js"]
}
